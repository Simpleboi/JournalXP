{
  "name": "backend",
  "version": "1.0.0",
  "description": "This is the backend folder for the JournalXP platform",
  "type": "module",
  "main": "server.ts",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon --watch src -e ts,json --exec \"node -r ts-node/register -r tsconfig-paths/register src/server.ts\"",
    "build": "tsc && tsc-alias",
    "start": "node dist/server.js"
  },
  "keywords": [
    "backend",
    "JournalXP"
  ],
  "author": "Nathaniel Paz",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "firebase-admin": "^13.5.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0"
  }
}
