{
  "expo": {
    "name": "JournalXP",
    "slug": "journalxp",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#6366f1"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.journalxp.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.journalxp.app"
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router"
    ],
    "scheme": "journalxp",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "d52829da-90e1-48a2-a857-2e153af42506"
      }
    }
  }
}
